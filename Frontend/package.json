{
  "name": "ng-music",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:portal": "nx serve portal --port=4200 --open --hmr",
    "start:admin": "nx serve admin --port=4201 --open --hmr",
    "build:all": "nx run-many --target=build --parallel=10 --prod",
    "build:affected": "nx affected --target=build --parallel=10 --prod",
    "build:portal": "nx build portal --prod",
    "build:admin": "nx build admin --prod",
    "e2e:portal": "nx e2e portal-e2e --watch",
    "e2e-ci:portal": "nx e2e portal-e2e",
    "e2e:admin": "nx e2e admin-e2e --watch",
    "e2e-ci:admin": "nx e2e admin-e2e",
    "dep-graph": "nx dep-graph",
    "lint:html": "htmlhint libs/**/*.html apps/**/*.html",
    "lint:scss": "stylelint {libs,apps}/**/*.scss",
    "lint:ts": "nx affected --target=lint --parallel=10",
    "format": "prettier --write --cache \"{libs,apps}/**/*.{ts,scss,md,js,json,html}\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~15.1.0",
    "@angular/common": "~15.1.0",
    "@angular/compiler": "~15.1.0",
    "@angular/core": "~15.1.0",
    "@angular/forms": "~15.1.0",
    "@angular/platform-browser": "~15.1.0",
    "@angular/platform-browser-dynamic": "~15.1.0",
    "@angular/router": "~15.1.0",
    "@nrwl/angular": "15.7.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.1.0",
    "@angular-devkit/core": "^15.1.6",
    "@angular-devkit/schematics": "^15.1.6",
    "@angular-eslint/eslint-plugin": "~15.0.0",
    "@angular-eslint/eslint-plugin-template": "~15.0.0",
    "@angular-eslint/template-parser": "~15.0.0",
    "@angular/cli": "~15.1.0",
    "@angular/compiler-cli": "~15.1.0",
    "@angular/language-service": "~15.1.0",
    "@nrwl/cypress": "15.7.2",
    "@nrwl/eslint-plugin-nx": "15.7.2",
    "@nrwl/jest": "15.7.2",
    "@nrwl/linter": "15.7.2",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/workspace": "15.7.2",
    "@schematics/angular": "^15.1.6",
    "@types/jest": "28.1.1",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "cypress": "^12.2.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "htmlhint": "^1.1.4",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "jest-preset-angular": "~12.2.3",
    "nx": "15.7.2",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-attributes": "0.0.5",
    "stylelint": "^ 14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard-scss": "^6.1.0",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.8.2"
  }
}
